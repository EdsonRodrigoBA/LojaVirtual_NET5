@model Colaboradores
<form asp-action="Cadastrar" method="post">
    <div class="card">
        <header class="card-header">
            <h4 class="card-title mt-2">Cadastrar Categoria</h4>
        </header>



        <div class="col form-group">
            <label asp-for="nome"></label>
            <input type="text" class="form-control" asp-for="nome" placeholder="Nome">
            <span class="text-danger" asp-validation-for="nome">  </span>
        </div>
        <div class="col form-group">
            <label asp-for="email"></label>
            <input type="text" class="form-control" asp-for="email" placeholder="E-mail">
            <span class="text-danger" asp-validation-for="email">  </span>
        </div>



        <div class="col form-group">
            <button type="submit" class="btn btn-primary"> Salvar </button>
            <a asp-action="Index" class="btn btn-warning"> Voltar </a>

        </div>
    </div>
</form>